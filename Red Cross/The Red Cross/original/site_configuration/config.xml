	<!-- PAGE 2 -->
	<page>
		<name>BG OPTIONS</name>
		<hasSubPages>true</hasSubPages>
		<subPage>
			<!-- title sets the title in the top left of the browser window -->
			<name>BACKGROUND - TILED IMAGE</name>
			<title>TILED BACKGROUND IMAGE</title>
			<xmlPath>site_configuration/tiledImageBackground.xml</xmlPath>
			<cssPath>site_stylesheets/content.css</cssPath>
			<utilityType>content</utilityType>
			<muteAudio>false</muteAudio>
			<backgroundOptions>
				<topMargin>0</topMargin>
				<bottomMargin>0</bottomMargin>
				<useImage>true</useImage>
				<imageURL>site_images/backgroundImages/whiteBlackDot.gif</imageURL>
